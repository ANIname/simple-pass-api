{
  "name": "simple-pass-api",
  "version": "1.0.0",
  "description": "Api for simple password manager",
  "private": true,
  "main": "src",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node dist",
    "dev": "ts-node-dev --respawn src",
    "linter": "eslint --fix src",
    "test": "npx jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "engines": {
    "node": "^12.18.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ANIname/simple-pass-api.git",
    "bugs": "https://github.com/ANIname/simple-pass-api/issues",
    "info": "https://github.com/ANIname/simple-pass-api#readme"
  },
  "keywords": [
    "api",
    "simple",
    "password",
    "manager"
  ],
  "author": "https://github.com/KiiDii",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-optimize-regex": "^1.2.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-unicorn": "^21.0.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.61",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "directory-import": "^1.2.1",
    "fastify": "^3.3.0",
    "fastify-swagger": "^3.3.0",
    "lodash": "^4.17.20"
  }
}
